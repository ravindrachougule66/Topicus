          <SQL_FORM DB="ORACLE-SEP" RESTAB="Memory">
            <SQL_STMT_U val="select max(r.rr_periode_tot)&#xA;  from rente_renseignering r, rekening_renteconditie_vw rc&#xA; where r.rr_geboekt between 1 and 2&#xA;       and r.rr_proforma = 0&#xA;       and r.rr_periode_tot >= ~1&#xA;       and r.rr_rekeningmuntsoort=~2&#xA;      &#xA;       and rc.rr_relatienummer = r.rr_relatienummer&#xA;       and rc.rr_rekeningnummer = r.rr_rekeningnummer&#xA;       and rc.rr_rekeningsoort = r.rr_rekeningsoort&#xA;       and rc.rr_rekeningmuntsoort = r.rr_rekeningmuntsoort&#xA;       and rc.rr_begindatum &lt;= r.rr_periode_tot&#xA;       and rc.rr_einddatum >= r.rr_periode_vanaf&#xA;      &#xA;       and exists (select 1&#xA;          from rekeningen rk&#xA;         where rk.re_nummer = r.rr_relatienummer&#xA;               and rk.re_rekening = r.rr_rekeningnummer&#xA;               and rk.re_soort = r.rr_rekeningsoort&#xA;               and rk.re_muntsoort = r.rr_rekeningmuntsoort&#xA;               and rk.re_status between 0 and 8)&#xA;      &#xA;       and ((rc.rr_rente_afspraak > 0 and&#xA;       rc.rr_rente_afspraak in&#xA;       (Select ra.inag_nummer&#xA;                from re_interest_agreement_vw ra&#xA;               where ra.inag_muntsoort = r.rr_rekeningmuntsoort&#xA;                     and (ra.inag_debet_rente = ~3 or&#xA;                     ra.inag_credit_rente = ~3)))&#xA;       &#xA;       or (rc.rr_rente_schaaltarief > 0 and&#xA;       rc.rr_rente_schaaltarief in&#xA;       (Select rs.insd_code&#xA;                   from re_interest_scale_dtl_vw rs&#xA;                  where rs.insd_muntsoort = r.rr_rekeningmuntsoort&#xA;                        and rs.insd_basistarief = ~3)))"/>
