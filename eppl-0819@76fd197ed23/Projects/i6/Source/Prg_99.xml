          <SQL_FORM DB="ORACLE-SEP" RESTAB="Memory">
            <SQL_STMT_U val="insert into WERKBESTAND  &#xA;  (WK_TERMINAL,&#xA;   WK_SOORT,&#xA;   WK_CATEGORIE_1,&#xA;   WK_CATEGORIE_2,&#xA;   WK_CATEGORIE_4,&#xA;   WK_CATEGORIE_3,&#xA;   WK_WAARDE_1,&#xA;   WK_WAARDE_2,&#xA;   WK_WAARDE_3,&#xA;   WK_WAARDE_4) &#xA;  (select distinct ~1,&#xA;          'VRTX', &#xA;          r.re_nummer, /* WK_CATEGORIE_1 */&#xA;          r.re_rekening, /* WK_CATEGORIE_2 */&#xA;          r.re_muntsoort, /* WK_CATEGORIE_4 */&#xA;          h.hpp_holdernummer, /* WK_CATEGORIE_3 */&#xA;          h.hpp_type_holder, /* WK_WAARDE_1 */&#xA;          nvl(a.ap_saldo_positie, 0), /* WK_WAARDE_2 */&#xA;          a.ap_transnr_transdatum , /* WK_WAARDE_3 */&#xA;          r.re_soort /* WK_WAARDE_4 */&#xA;     from REKENINGEN r, ON_LINE_DOSSIER o, HOLDERS_PER_PRODUCT h, TAP_AKTUELE_POSITIES_VW01 a &#xA;    where r.re_nummer = o.onld_relatienummer&#xA;      and o.onld_relatienummer = h.hpp_relatienummer&#xA;      and r.re_nummer = a.ap_relatienr(+)&#xA;      and r.re_rekening = a.ap_rekeningnr(+)&#xA;      and r.re_muntsoort = a.ap_rekening_munts(+)&#xA;      and r.re_soort = a.ap_rekening_soort(+)&#xA;      and h.hpp_relatienummer = r.re_nummer&#xA;      and h.hpp_product = o.onld_productnummer&#xA;      and h.hpp_product_volgnr = o.onld_product_volgnummer&#xA;      and o.onld_klasse = 40 /*We kijken of er een klasse 40 record bestaat (Effectenkrediet is JA)*/&#xA;      and o.onld_ontvangstdatum &lt;> '00000000' /*Tevens moet hij op retour staan*/&#xA;      and r.RE_REKENING_STATUS = 2&#xA;      and r.re_soort between 1000 and 1199 &#xA;      and exists&#xA;    (select 1&#xA;             from REKENINGEN_PER_PRODUCT rp&#xA;            where rp.rpp_productnummer = o.onld_productnummer&#xA;              and rp.rpp_volgnr_per_product = o.onld_product_volgnummer&#xA;              and rp.rpp_rekeningsoort between 100 and 999&#xA;              and rp.rpp_relatienummer = r.re_nummer&#xA;              and rp.rpp_rekening_nummer = r.re_rekening))&#xA;order by r.re_nummer, r.re_rekening, r.re_soort, r.re_muntsoort"/>
            <INARG>
