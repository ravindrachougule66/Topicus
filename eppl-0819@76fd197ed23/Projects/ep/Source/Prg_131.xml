          <SQL_FORM DB="ORACLE-SEP" RESTAB="Memory">
            <SQL_STMT_U val="INSERT INTO special_ca_tax_exclusions&#xD;&#xA;  (PARTY_ID, DEDU_ID)&#xD;&#xA;  SELECT A.PARTY_ID, A.DEDUCTION_ID&#xD;&#xA;    FROM deduction_exemptions_vw A,&#xD;&#xA;         (SELECT C.CFCU_DEDU_ID&#xD;&#xA;            FROM ca_tax_exclusions B&#xD;&#xA;           INNER JOIN calculationrules C&#xD;&#xA;              ON C.CFCU_ID = B.CTEX_CFCU_ID&#xD;&#xA;           INNER JOIN Contextcalculationrules ccr&#xD;&#xA;              on ccr.cxcr_cfcu_id = c.cfcu_id&#xD;&#xA;             and ccr.cxcr_context = 'CA'&#xD;&#xA;             and (ccr.cxcr_ts_indicatienummer = ~5 OR (ccr.cxcr_ts_indicatienummer = ~6 AND ~6&lt;>0) ) --pi-_Transactie_indicatie &#xD;&#xA;           WHERE B.CTEX_RA_ID = ~1 --pi-_RA_Id&#xD;&#xA;             AND B.CTEX_EXCLUSION_LEVEL = 1) D&#xD;&#xA;   WHERE A.DEDUCTION_ID = D.CFCU_DEDU_ID&#xD;&#xA;     AND A.PARTY_ID IN&#xD;&#xA;         (SELECT G.HPP_HOLDERNUMMER&#xD;&#xA;            FROM rekeningen E&#xD;&#xA;           INNER JOIN rekeningen_per_product F&#xD;&#xA;              ON F.RPP_RELATIENUMMER = E.RE_NUMMER&#xD;&#xA;             AND F.RPP_REKENING_NUMMER = E.RE_REKENING&#xD;&#xA;             AND F.RPP_REKENINGSOORT = E.RE_SOORT&#xD;&#xA;             AND F.RPP_VALUTA = E.RE_MUNTSOORT&#xD;&#xA;           INNER JOIN holders_per_product G&#xD;&#xA;              ON G.HPP_RELATIENUMMER = F.RPP_RELATIENUMMER&#xD;&#xA;             AND G.HPP_PRODUCT = F.RPP_PRODUCTNUMMER&#xD;&#xA;             AND G.HPP_PRODUCT_VOLGNR = F.RPP_VOLGNR_PER_PRODUCT&#xD;&#xA;             AND G.HPP_TYPE_HOLDER IN (1, 2)&#xD;&#xA;           WHERE E.RE_ID IN&#xD;&#xA;                 (SELECT CASE&#xD;&#xA;                           WHEN J.VO_RELATIE IS NULL THEN&#xD;&#xA;                            H.AP_RE_ID&#xD;&#xA;                           ELSE&#xD;&#xA;                            K.RE_ID&#xD;&#xA;                         END&#xD;&#xA;                    FROM tap_aktuele_posities_vw01 H&#xD;&#xA;                   CROSS JOIN renteschemas I&#xD;&#xA;                    LEFT JOIN VRUCHTGEBR_OPBRENGST J&#xD;&#xA;                      ON J.VO_RELATIE = H.AP_RELATIENR&#xD;&#xA;                     AND J.VO_REKENING = H.AP_REKENINGNR&#xD;&#xA;                     AND J.VO_REKENINGSOORT = H.AP_REKENING_SOORT&#xD;&#xA;                     AND J.VO_MUNTSOORT = H.AP_REKENING_MUNTS&#xD;&#xA;                     AND J.VO_SOORT = CASE&#xD;&#xA;                           WHEN I.RA_TYPE_RB_NA_VA = 'RB' THEN&#xD;&#xA;                            'COUP'&#xD;&#xA;                           WHEN H.AP_BI_TYPE BETWEEN 100 AND 199 THEN&#xD;&#xA;                            'CDIV'&#xD;&#xA;                         END&#xD;&#xA;                    LEFT JOIN REKENINGEN K&#xD;&#xA;                      ON K.RE_NUMMER = J.VO_VRUCHTRELATIE&#xD;&#xA;                     AND K.RE_REKENING = J.VO_VRUCHTREKENING&#xD;&#xA;                     AND K.RE_SOORT BETWEEN 1 AND 999&#xD;&#xA;                     AND K.RE_MUNTSOORT = ' '&#xD;&#xA;                   WHERE H.AP_RELATIENR BETWEEN ~3 AND ~4 --pi-_Relatie_VAN + pi-_Relatie_TOT&#xD;&#xA;                     AND H.AP_REKENING_SOORT BETWEEN 1 AND 999&#xD;&#xA;                     AND H.AP_BE_VOLGNUMMER = ~2 --pi-_BE_Volgnummer&#xD;&#xA;                     AND (H.AP_SALDO_POSITIE &lt;> 0 OR H.AP_POSITIE_LONG &lt;> 0 OR&#xD;&#xA;                         H.AP_POSITIE_SHORT &lt;> 0)&#xD;&#xA;                     AND I.RA_ID = ~1)) --pi-_RA_Id&#xD;&#xA;     AND A.PARTY_ID &lt;> 0&#xD;&#xA;     AND A.DEDUCTION_ID IS NOT NULL&#xD;&#xA;"/>
